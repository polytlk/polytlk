environments:
  default: {}
  development: {}
---
repositories:
- name: open-telemetry
  url: https://open-telemetry.github.io/opentelemetry-helm-charts

releases:
- name: opentelemetry-collector
  namespace: opentelemetry
  chart: open-telemetry/opentelemetry-collector
  version: 0.78.2
  installed: true
  values:
  - otel-collector-{{ .Environment.Name }}-config.yaml
